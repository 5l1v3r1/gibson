logfile      /dev/null
loglevel     0
logflushrate 1
unix_socket  gibson.sock
daemonize 1
pidfile   /tmp/gibson.pid
max_memory       1G
max_item_ttl     2592000
max_idletime     5
max_clients      255
max_request_size 10M
max_key_size 512K
max_value_size 9M
max_response_size 15M
compression 500
cron_period      100
